library(tidyverse)
library(ggplot2)
library(ggtext)
theme_set(theme_classic())
files <- fs::dir_ls(recurse = TRUE, glob = "data/oaincubationsos*.csv")
merged_data <- files %>% purrr::map_df(., readr::read_csv, .id = "filename")
library(dplyr)
merged_data %>%
ggplot(aes(x = Genotype, y = Response.time)) +
geom_boxplot() +
geom_point(aes(color = Condition)) +
labs(title = "4mM OA PRE-incubation of N2, cest-2.1, and tbh-1 for 30% octanol avoidance") +
facet_grid(~Condition) +
scale_fill_manual(values = c("green", "grey", "blue", "red", "purple", "black", "orange", "yellow", "brown", "pink")) +
scale_color_manual(values = c("green", "grey", "blue", "red", "purple", "black", "orange", "yellow", "brown", "pink"))
